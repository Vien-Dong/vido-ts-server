{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "redirects": [
    {
      "source": "/:path*",
      "destination": "https://vido-ts-server.vercel.app/:path*",
      "permanent": true
    }
  ]
}